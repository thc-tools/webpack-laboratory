{
  "name": "@thc/webpack-react",
  "version": "0.1.0-alpha.8",
  "description": "Ready to use webpack conf for React",
  "scripts": {
    "pre-release": "standard-version",
    "pre-release-alpha": "standard-version --prerelease alpha",
    "pre-release-beta": "standard-version --prerelease beta",
    "release": "npm publish --access public && conventional-github-releaser -p angular && git push --follow-tags origin master",
    "release-alpha": "npm publish --tag alpha --access public && conventional-github-releaser -p angular && git push --follow-tags origin master",
    "release-beta": "npm publish --tag beta --access public && conventional-github-releaser -p angular && git push --follow-tags origin master",
    "test": "jest --runInBand --no-cache",
    "postinstall": "node ./postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thc-tools/webpack-react.git"
  },
  "keywords": [
    "thc",
    "template"
  ],
  "author": "THC-Tools",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thc-tools/webpack-react/issues"
  },
  "homepage": "https://github.com/thc-tools/webpack-react#readme",
  "peerDependencies": {
    "@thc/babel-preset-react": "0.1.0-alpha.*",
    "webpack": "4.x"
  },
  "devDependencies": {
    "conventional-github-releaser": "3.1.2",
    "jest": "23.4.1",
    "standard-version": "4.4.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "webpack": "4.16.1"
  },
  "dependencies": {
    "babel-loader": "8.0.0-beta.4",
    "cache-loader": "1.2.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "cssnano": "4.0.1",
    "eslint-loader": "2.0.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.3",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.6",
    "postcss-mixins": "6.2.0",
    "postcss-normalize": "6.0.0",
    "postcss-preset-env": "5.2.1",
    "react-dev-utils": "5.0.1",
    "sass-loader": "7.0.3",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "thread-loader": "1.1.5",
    "ts-loader": "4.4.2",
    "url-loader": "1.0.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "whatwg-fetch": "2.0.4"
  }
}
