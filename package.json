{
  "name": "@thc/webpack-react",
  "version": "1.0.0",
  "description": "Ready to use webpack conf for React",
  "scripts": {
    "lerna": "./node_modules/.bin/lerna",
    "bootstrap": "npm run lerna -- bootstrap --no-ci",
    "test": "jest --runInBand --no-cache",
    "push-tags": "git push --follow-tags origin master",
    "beta-pre-release-lerna": "npm run lerna -- version --no-push prerelease --preid beta --exact",
    "pre-release-lerna": "npm run lerna -- version --no-push --exact",
    "beta-release-lerna": "npm run lerna -- publish from-git --npm-tag beta",
    "release-lerna": "npm run lerna -- publish from-git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thc-tools/webpack-react.git"
  },
  "keywords": [
    "thc",
    "webpack",
    "configuration"
  ],
  "author": "THC-Tools",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thc-tools/webpack-react/issues"
  },
  "homepage": "https://github.com/thc-tools/webpack-react#readme",
  "devDependencies": {
    "jest": "23.6.0",
    "lerna": "3.8.5",
    "webpack": "4.27.1"
  }
}
